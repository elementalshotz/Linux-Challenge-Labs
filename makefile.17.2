CC = gcc
CFLAGS = -c -Wall
EXE = rovarsprak.exe
CLEAN_OBJ = *.o
OBJECTS = funclib.o getch_linux.o rovarsprak.o

all: $(OBJECTS) output clean

output: $(OBJECTS)
	$(CC) $(OBJECTS) -o $(EXE)

$(OBJECTS): %.o: %.c $(wildcard %.h)
	$(CC) $(CFLAGS) $<

clean:
	rm -f $(CLEAN_OBJ)

realclean: clean
	rm -f $(EXE)

target: dependencies
	action
